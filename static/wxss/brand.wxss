.brand {
  min-height: 100vh;
  padding: 20rpx;
  box-sizing: border-box;
  background: #f4f4f4;
}
.uls {
  display: flex;
  flex-wrap: wrap;
}
.lis {
  width: 229rpx;
  height: 173rpx;
  border-radius: 15rpx;
  position: relative;
  background: red;
  margin-bottom: 15rpx;
  margin-right: 10rpx;
}
.lis:nth-child(3n) {
  margin-right: 0;
}
.lis img {
  width: 100%;
  height: 100%;
}